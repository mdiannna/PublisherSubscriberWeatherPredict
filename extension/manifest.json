 {
    "name": "Getting Started Example",
    "version": "2.0",
    "description": "Build an Extension!",
    "permissions": ["activeTab", "declarativeContent", "storage", "<all_urls>"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
      "default_popup": "html/popup.html",
       "default_icon": {
        "16": "images/weather_icon.png",
        "32": "images/weather_icon.png",
        "48": "images/weather_icon.png",
        "128": "images/weather_icon.png"
      },
      "default_title": "Wether predict Extension!"
    },

    "icons": {
    	"16": "images/weather_icon.png",
        "32": "images/weather_icon.png",
        "48": "images/weather_icon.png",
        "128": "images/weather_icon.png"
    },

    "manifest_version": 2
  
   
  }